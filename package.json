{
  "name": "imageless",
  "version": "1.0.2",
  "description": "Compress the images in the project without invasion and back up the source files",
  "keywords": [
    "imagemin",
    "compress",
    "image"
  ],
  "main": "index.js",
  "scripts": {
    "start": "pm2 start ./watchlocal.js -n tinify",
    "restart": "pm2 restart tinify"
  },
  "type": "module",
  "bin": {

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangcylive/imageless.git"
  },
  "author": "wangcylive",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wangcylive/imageless/issues"
  },
  "homepage": "https://github.com/wangcylive/imageless#readme",
  "dependencies": {
    "chokidar": "^3.5.3",
    "image-size": "^1.0.0",
    "imagemin": "^8.0.1",
    "imagemin-jpeg-recompress": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "make-dir": "^3.1.0",
    "md5": "^2.3.0",
    "tinify": "^1.7.1"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "index.js",
    "utils.js",
    "imageless.js",
    "package.json",
    "README.md"
  ],
  "imageless": {
    "input": [
      "src/**/*.jpg"
    ],
    "jpegOptions": {
      "quality": 70
    },
    "pngOptions": {
      "quality": [
        0.6,
        0.8
      ]
    }
  }
}
