{
  "name": "imageless",
  "version": "1.0.2",
  "description": "Compress the images in the project without invasion and back up the source files",
  "keywords": [
    "imagemin",
    "compress",
    "image"
  ],
  "main": "index.js",
  "scripts": {
    "start": "pm2 start ./dist/watchlocal.js -n tinify",
    "restart": "pm2 restart tinify",
    "webpack": "webpack --config ./config/webpack-run.js",
    "build": "NODE_ENV=production npm run webpack"
  },
  "bin": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangcylive/imageless.git"
  },
  "author": "wangcylive",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wangcylive/imageless/issues"
  },
  "homepage": "https://github.com/wangcylive/imageless#readme",
  "dependencies": {
    "chokidar": "^3.5.3",
    "image-size": "^1.0.0",
    "imagemin": "^8.0.1",
    "imagemin-jpeg-recompress": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "make-dir": "^3.1.0",
    "md5": "^2.3.0",
    "tinify": "^1.7.1"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "index.js",
    "utils.js",
    "imageless.js",
    "package.json",
    "README.md"
  ],
  "imageless": {
    "input": [
      "src/**/*.jpg"
    ],
    "jpegOptions": {
      "quality": 70
    },
    "pngOptions": {
      "quality": [
        0.6,
        0.8
      ]
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/plugin-transform-runtime": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "babel-loader": "^9.1.3",
    "babel-plugin-import": "^1.13.8",
    "cross-env": "^7.0.3",
    "internal-ip": "^8.0.0",
    "nodemon": "^3.0.1",
    "portfinder": "^1.0.32",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-merge": "^5.10.0",
    "webpack-nano": "^1.1.1",
    "webpack-node-externals": "^3.0.0"
  }
}
